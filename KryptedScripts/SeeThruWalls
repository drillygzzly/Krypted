local CameraSel = "Invisicam"
local CameraMax = "696969"
local CameraMin = "0"

game.Players.LocalPlayer.DevCameraOcclusionMode = CameraSel
print("Camera Set To :", CameraSel)
game.Players.LocalPlayer.CameraMaxZoomDistance = CameraMax
